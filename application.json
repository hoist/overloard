  "out_file": "/var/log/node/overlord-tasks.stdout.log",
  "instances": "1",
  "merge_logs": false,
  "exec_mode": "fork_mode",
  "env": {
    "NODE_ENV": "production"
  }
},{
  "name": "overlord-web",
  "script": "./web_server.js",
  "cwd": "./",
  "error_file": "/var/log/node/overlord-web.stderr.log",
  "out_file": "/var/log/node/overlord-web.stdout.log",
  "instances": "1",
  "merge_logs": false,
  "exec_mode": "fork_mode",
  "env": {
    "NODE_ENV": "production"
  }
}]
